{
    "clipper_conf": {
        "batching": {
            "batch_size": 1, 
            "strategy": "static"
        }, 
        "num_benchmark_requests": 100000, 
        "num_message_encodes": 1, 
        "name": "mnist-ensemble_160830-124714", 
        "bench_batch_size": 1, 
        "models": [
            {
                "version": 1, 
                "num_outputs": 1, 
                "name": "kernel_svm", 
                "addresses": [
                    "kernel_svm_r0:6001", 
                    "kernel_svm_r1:6001"
                ]
            }
        ], 
        "input_type": "float", 
        "redis_port": 6379, 
        "target_qps": 100, 
        "results_path": "/tmp/benchmarking_logs", 
        "mnist_path": "/mnist_data/test.data", 
        "num_update_workers": 1, 
        "window_size": -1, 
        "num_predict_workers": 8, 
        "redis_ip": "redis", 
        "cache_size": 49999, 
        "slo_micros": 20000, 
        "correction_policy": "logistic_regression", 
        "salt_cache": true, 
        "use_lsh": false, 
        "input_length": 784
    }, 
    "docker_compose_conf": {
        "services": {
            "quantilereg": {
                "cpuset": "1-1", 
                "image": "clipper/quantile-reg"
            }, 
            "clipper": {
                "cpuset": "2-6", 
                "depends_on": [
                    "redis", 
                    "quantilereg", 
                    "kernel_svm_r0", 
                    "kernel_svm_r1"
                ], 
                "image": "cl-dev-digits", 
                "volumes": [
                    "${MNIST_PATH}:/mnist_data:ro", 
                    "${CLIPPER_ROOT}/digits_bench.toml:/tmp/digits_bench.toml:ro", 
                    "${CLIPPER_ROOT}/benchmarking_logs/resource-alloc:/tmp/benchmarking_logs"
                ]
            }, 
            "kernel_svm_r0": {
                "cpuset": "7-7", 
                "image": "clipper/sklearn-mw-dev", 
                "volumes": [
                    "${CLIPPER_ROOT}/model_wrappers/python/sklearn_models/svm_pred3/:/model:ro"
                ]
            }, 
            "kernel_svm_r1": {
                "cpuset": "8-8", 
                "image": "clipper/sklearn-mw-dev", 
                "volumes": [
                    "${CLIPPER_ROOT}/model_wrappers/python/sklearn_models/svm_pred3/:/model:ro"
                ]
            }, 
            "redis": {
                "cpuset": "0-0", 
                "image": "redis:alpine"
            }
        }, 
        "version": "2"
    }
}